    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'advent09'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=advent09",
                    "--package=advent09"
                ],
                "filter": {
                    "name": "advent09",
                    "kind": "bin"
                }
            },
            "args": [
                "example3"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'advent09'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=advent09",
                    "--package=advent09"
                ],
                "filter": {
                    "name": "advent09",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug benchmark 'bench'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bench=bench",
                    "--package=advent09"
                ],
                "filter": {
                    "name": "bench",
                    "kind": "bench"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}
